freeze;

declare type ExactpAdics_Store;
declare attributes ExactpAdics_Store: nextId;
STORE := New(ExactpAdics_Store);
STORE`nextId := 0;

///hide
intrinsic ExactpAdics_NextId() -> .
  {The next available id.}
  s := STORE;
  id := s`nextId + 1;
  s`nextId := id;
  return id;
end intrinsic;
